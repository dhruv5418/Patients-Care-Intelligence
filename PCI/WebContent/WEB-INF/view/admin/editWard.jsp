                                <h4 class="card-title">Edit Ward</h4>
                                <f:form class="form-material form-horizontal"  modelAttribute="data" action="update.html">
                                   <f:hidden path="wardId"/> 
                                    
                                    <div class="form-group">
                                        <h5>Ward Type <span class="text-danger">*</span></h5>
                                        <div class="controls">
                                            <f:input path="wardType" type="text" name="text" class="form-control" required="required" data-validation-required-message="This field is required"/> </div>
                                        <div class="form-control-feedback"></div>
                                    </div>
                                    
                                    <div class="form-group">
                                        <h5>Ward Number <span class="text-danger">*</span></h5>
                                        <div class="controls">
                                            <f:input path="wardName" name="text" class="form-control" required="required" data-validation-required-message="This field is required"/> </div>
                                        <div class="form-control-feedback"></div>
                                    </div>
                                    
                                    
                                    
                                    
                                    
                              
                                    
                                    <div class="text-xs-right">
                                        <button type="submit" class="btn btn-info">Submit</button>
